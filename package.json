    "README-cn.md",
    "example.html",
    "dist/miment.min.js",
    "lib/miment.js",
    "lib/miment.js.map"
  ],
  "repository": {
    "url": "https://github.com/noahlam/Miment.git",
    "type": "git"
  },
  "author": "noahlam <284516433@qq.com>",
  "license": "MIT",
  "scripts": {
    "build:cjs": "rollup -c ./build/rollup-cjs.js",
